<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import org.jfree.chart.fx.ChartViewer?>
<?import javafx.scene.control.Button?>
<VBox alignment="TOP_CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="fri.uniza.semestralka1.ui.GuiController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ChartViewer fx:id="chartViewer" prefWidth="600" prefHeight="550" maxWidth="600" maxHeight="550"/>
    <Button text="Start simulation" onAction="#startSimulation"/>
</VBox>
